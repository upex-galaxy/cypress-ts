{
	"name": "cypress-ts",
	"description": "cypress typescript",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"test": "cypress open",
		"rm:reports": "rm reports/* || true",
		"file": "cypress run --browser electron --reporter cypress-multi-reporters --reporter-options configFile=cytest.json --spec",
		"regression": "cypress run --browser electron --reporter cypress-multi-reporters --reporter-options configFile=cyregression.json",
		"regression:ci": "cypress run --browser electron --reporter cypress-multi-reporters --reporter-options configFile=cyregression.json --spec",
		"regression:report": "cypress run --browser electron --reporter mochawesome --reporter-options reportDir=mochawesome-report,overwrite=false,html=false,json=true",
		"report:xml": "jrm reports/regression-report.xml \"reports/*.xml\"",
		"report:json": "mochawesome-merge mochawesome-report/*.json>mochawesome.json",
		"report:html": "marge mochawesome.json",
		"allure:report": "allure generate reports/allure-results -o reports/allure-report --clean && allure open reports/allure-report --host localhost --port 9090",
		"allure:ci": "allure generate reports/allure-results -o reports/allure-report --clean",
		"prettier:write": "npx prettier --write cypress",
		"prettier:check": "npx prettier --check cypress",
		"lint:check": "npx eslint cypress",
		"lint:fix": "npx eslint --fix cypress"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/upex-galaxy/cypress-ts.git"
	},
	"keywords": ["cypress", "typescript", "upex"],
	"author": "upex",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/upex-galaxy/cypress-ts/issues"
	},
	"homepage": "https://github.com/upex-galaxy/cypress-ts#readme",
	"devDependencies": {
		"@bahmutov/cypress-esbuild-preprocessor": "2.2.0",
		"@types/node": "20.10.4",
		"@typescript-eslint/eslint-plugin": "6.14.0",
		"@typescript-eslint/parser": "6.14.0",
		"cypress": "13.6.1",
		"cypress-plugin-api": "2.11.1",
		"dotenv": "16.3.1",
		"esbuild": "0.19.9",
		"eslint": "8.55.0",
		"eslint-plugin-only-warn": "1.1.0",
		"prettier": "3.1.1",
		"typescript": "5.3.3",
		"@4tw/cypress-drag-drop": "2.2.5",
		"@badeball/cypress-cucumber-preprocessor": "19.2.0",
		"@esbuild-plugins/node-modules-polyfill": "0.2.2",
		"@faker-js/faker": "8.3.1",
		"@shelex/cypress-allure-plugin": "2.40.1",
		"allure-commandline": "2.25.0",
		"cypress-downloadfile": "1.2.3",
		"cypress-file-upload": "5.0.8",
		"cypress-mochawesome-reporter": "3.7.0",
		"cypress-multi-reporters": "1.6.4",
		"cypress-plugin-steps": "1.1.1",
		"cypress-recurse": "1.35.2",
		"cypress-xpath": "2.0.1",
		"junit-report-merger": "6.0.3",
		"mocha": "10.2.0",
		"mocha-junit-reporter": "2.2.1",
		"mochawesome": "7.1.3",
		"mochawesome-merge": "4.3.0",
		"mochawesome-report-generator": "6.2.0",
		"multiple-cucumber-html-reporter": "3.5.0"
	}
}
